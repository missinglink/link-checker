{
  "name": "link-checker",
  "preferGlobal": "fasle",
  "private": true,
  "version": "0.0.1",
  "description": "LinkChecker",
  "contributors": [
    {
      "name": "Fabrizio Moscon",
      "email": "mosconfabrizio@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/missinglink/link-checker"
  },
  "scripts": {
    "start": "export NODE_PATH=`pwd` && coffee src/server/app.coffee",
    "front-test": "export NODE_PATH=`pwd` && mocha public/test --recursive --reporter spec --compilers coffee:coffee-script",
    "back-test": "export NODE_PATH=`pwd` && mocha test --recursive --reporter spec --compilers coffee:coffee-script",
    "test": "export NODE_PATH=`pwd` && npm run back-test && npm run front-test"
  },
  "dependencies": {
    "mongodb": "1.3.x",
    "coffee-script": "1.6.x",
    "redis": "0.8.x",
    "socket.io": "0.9.x",
    "validator": "0.4.x",
    "check-types": "0.6.x"
  },
  "devDependencies": {
    "mocha": "1.9.x",
    "assert": "0.4.x",
    "should": "1.2.x",
    "breakdown": "*"
  },
  "engines": {
    "node": "0.8.x",
    "npm": "1.1.x",
    "mongo": ">=2.2"
  }
}