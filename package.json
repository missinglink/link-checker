{
  "name": "link-checker",
  "preferGlobal": "true",
  "version": "0.0.1",
  "scripts": {
    "start": "coffee src/server/app.coffee",
    "front-test": "npm run prestart && mocha public/test --recursive --reporter spec --compilers coffee:coffee-script",
    "back-test": "mocha test --recursive --reporter spec --compilers coffee:coffee-script",
    "test": "npm run back-test && npm run front-test"
  },
  "dependencies" : {
    "coffee-script" : "*",
    "redis" : "*",
    "jitter" : "*",
    "socket.io" : "0.9.10",
    "should": "*",
    "validator": "0.4.16",
    "mongodb": "git://github.com/fabriziomoscon/node-mongodb-native#master"
  },
  "engines": {
    "node": "0.8.x",
    "npm": "1.1.x"
  }
}